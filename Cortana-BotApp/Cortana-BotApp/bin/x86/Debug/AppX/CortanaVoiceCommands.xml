<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="CustomCommandSet_en-us">
    <CommandPrefix>Dipa</CommandPrefix>
    <Example>hi</Example>

    <Command Name="sayHiToBot">
      <Example>hi</Example>
      <ListenFor>hi</ListenFor>
      <ListenFor>hello</ListenFor>
      <ListenFor>find me a hotel in london</ListenFor>
      <Feedback>Connecting to Bot</Feedback>
      <VoiceCommandService Target="VoiceCommandService" />
    </Command>
  </CommandSet>
  <CommandSet xml:lang="en-in" Name="CustomCommandSet_en-in">
    <CommandPrefix>Dipa</CommandPrefix>
    <Example>hi</Example>

    <Command Name="sayHiToBot">
      <Example>hi</Example>
      <ListenFor>hi</ListenFor>
      <ListenFor>hello</ListenFor>
      <ListenFor>find me a hotel in london</ListenFor>
      <Feedback>Connecting to Bot</Feedback>
      <VoiceCommandService Target="VoiceCommandService" />
    </Command>
  </CommandSet>
</VoiceCommands>